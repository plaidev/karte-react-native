{
  "name": "react-native-karte",
  "description": "KARTE SDK for React Native.",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/react-native",
      "**/react-native/**"
    ]
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/plaidev/karte-react-native.git"
  },
  "author": "PLAID, Inc.",
  "scripts": {
    "prepare": "lerna run prepare",
    "release": "lerna publish from-package",
    "release:canary": "lerna publish --canary",
    "typecheck": "tsc --noEmit --composite false",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "format": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "docs": "lerna run docs",
    "docs:clean": "lerna run docs:clean",
    "docs:publish": "yarn docs && ts-node scripts/publish-docs.ts",
    "test": "jest",
    "yalc:push": "yarn prepare && lerna exec --parallel -- yalc push --force"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@lerna-lite/cli": "^4.9.2",
    "@lerna-lite/exec": "^4.9.2",
    "@lerna-lite/publish": "^4.9.2",
    "@lerna-lite/run": "^4.9.2",
    "@react-native-community/eslint-config": "^3.1.0",
    "@react-native-community/cli": "^18.0.0",
    "@react-native-community/cli-platform-android": "^18.0.0",
    "@react-native-community/cli-platform-ios": "^18.0.0",
    "@react-native/babel-preset": "^0.79.6",
    "@react-native/eslint-config": "^0.79.6",
    "@react-native/metro-config": "^0.79.6",
    "@react-native/typescript-config": "^0.79.6",
    "@types/react-test-renderer": "^19.0.0",
    "react-test-renderer": "^19.0.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "compare-versions": "^3.6.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "exit-hook": "^2.2.1",
    "fs-extra": "^9.1.0",
    "genversion": "^2.2.1",
    "husky": "^4.2.5",
    "jest": "^29.6.3",
    "pod-install": "^0.1.8",
    "prettier": "^2.0.5",
    "react": "^19.0.0",
    "react-native": "^0.79.6",
    "ts-jest": "^26.5.4",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.17",
    "typescript": "~5.0.4"
  },
  "jest": {
    "projects": [
      "<rootDir>/packages/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn typecheck"
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "engines": {
    "node": ">=18"
  }
}
