{
  "name": "@react-native-karte/utilities",
  "version": "0.1.2",
  "description": "KARTE SDK Utilities",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "keywords": [
    "react-native",
    "karte",
    "utilities"
  ],
  "author": "PLAID, Inc.",
  "homepage": "https://karte.io",
  "license": "Apache-2.0",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plaidev/karte-react-native.git",
    "directory": "packages/utilities"
  },
  "scripts": {
    "prepare": "bob build",
    "build": "bob build",
    "build:clean": "del lib",
    "docs": "typedoc src/index.ts --excludePrivate",
    "docs:clean": "del docs",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/plaidev/karte-react-native/issues"
  },
  "devDependencies": {
    "react-native-builder-bob": "^0.18.3",
    "typescript": "~5.0.4"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
