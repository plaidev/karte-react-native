{
  "name": "@react-native-karte/visual-tracking",
  "version": "0.1.0",
  "description": "KARTE SDK Visual tracking for React Native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "keywords": [
    "react-native",
    "karte",
    "visual-tracking",
    "ios",
    "android"
  ],
  "author": "PLAID, Inc.",
  "homepage": "https://karte.io",
  "license": "Apache-2.0",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "RNKRTVisualTracking.podspec",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plaidev/karte-react-native.git",
    "directory": "packages/visual-tracking"
  },
  "scripts": {
    "prepare": "bob build",
    "build": "bob build",
    "build:clean": "del lib",
    "docs": "typedoc src/index.tsx --excludePrivate",
    "docs:clean": "del docs",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/plaidev/karte-react-native/issues"
  },
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-builder-bob": "*",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "@react-native-karte/core": "^0.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
